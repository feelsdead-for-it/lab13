#include <iostream>
#include <cmath>
using namespace std;
int main()
{

int  a,b,f,n;
	cin >>n;
	cin >>a;
	cin >>b;

	f = 0;
	int* m = new int[n];
	for (int i = 0; i < n; i++)
	{
		m[i] = a * pow(b, f);
		f++;
	}

	cout << "массив" << endl;
	for (int i = 0; i < n; i++)
	{
		cout << m[i] << "\t";
	}


}
